SELECT value as word, Phrases FROM json_each(GetParam('WordList')), SimilarPhrase where Phrases like '%'|| word || '%'</sql><sql name="SQL 8">SELECT SetParam('sentence', 'There once was a movie; it was called westside history something; maybe west-side story.');
SELECT SetParam('words', 'westside story movie');
/*
  If can trim the punctuation correctly, then could match on
  exact words rather than using LIKE if handle upper/lower case.
*/
with
sentence as 
(
 select trim(value,'!?;.') as word
 from json_each(json('[''' || replace(GetParam('sentence'),' ',''',''') || ''']'))
),
words as
(
 select value as word
 from json_each(json('[''' || replace(GetParam('words'),' ',''',''') || ''']'))
)
select 
   s.word as sent_words, w.word as search_words
from
   sentence s, words w
where
  s.word like '%'|| w.word || '%'
;</sql><sql name="SQL 9">/*
